def suma_par_impar():
    suma_par=0
    suma_impar=0
    for x in range(0,5):
        num=int(input("Dame un número: "))
        if(num%2==0):
            print("Tienes un número par")
            suma_par=suma_par+num
        else:
            print("Tienes un número impar")
            suma_impar=suma_impar+num
    print("La suma de los pares = "+str(suma_par))
    print("La suma de los impares = "+str(suma_impar))
suma_par_impar()
     
